{
  "name": "@bobheadxi/deployments",
  "version": "0.0.0",
  "private": true,
  "description": "GitHub Action for working painlessly with deployment statuses",
  "main": "src/main.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "pack": "ncc build",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run build && npm run pack && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobheadxi/deployments.git"
  },
  "author": "bobheadxi",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.4",
    "@actions/github": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.20",
    "@typescript-eslint/eslint-plugin": "^3.6.0",
    "@typescript-eslint/parser": "^3.6.0",
    "@zeit/ncc": "^0.22.3",
    "eslint": "^7.4.0",
    "eslint-plugin-jest": "^23.18.0",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "jest-circus": "^26.1.0",
    "js-yaml": "^3.14.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "typescript": "^3.9.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run all && git add ."
    }
  }
}
